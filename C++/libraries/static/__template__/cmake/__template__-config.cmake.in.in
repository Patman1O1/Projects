@PACKAGE_INIT@

# Helper macro to import the static target
macro(import_static_target)
    if(NOT EXISTS "${CMAKE_CURRENT_LIST_DIR}/@NAME@-static-targets.cmake")
        set(${CMAKE_FIND_PACKAGE_NAME}_NOT_FOUND_MESSAGE "@NAME@ static libraries were requested but not found")
        set(${CMAKE_FIND_PACKAGE_NAME}_FOUND OFF)
        return()
    endif()

    include("${CMAKE_CURRENT_LIST_DIR}/@NAME@-static-targets.cmake")
endmacro()

# Only import if not already defined
if(NOT TARGET @NAMESPACE@::@NAME@)
    import_static_target()
    check_required_components(@NAME@)
    message("-- Found static @NAME@ (version ${${CMAKE_FIND_PACKAGE_NAME}_VERSION})")
endif()
