@PACKAGE_INIT@

# Only import if not already defined
if(NOT TARGET ${NAMESPACE}::${PROJECT_NAME})
    if(NOT EXISTS "${CMAKE_CURRENT_LIST_DIR}/${PROJECT_NAME}-targets.cmake")
        set(${CMAKE_FIND_PACKAGE_NAME}_NOT_FOUND_MESSAGE "${PROJECT_NAME} libraries were requested but not found")
        set(${CMAKE_FIND_PACKAGE_NAME}_FOUND OFF)
        return()
    endif()
    
    include("${CMAKE_CURRENT_LIST_DIR}/${PROJECT_NAME}-targets.cmake")
    check_required_components(${PROJECT_NAME})
    message("-- Found ${PROJECT_NAME} (version ${${CMAKE_FIND_PACKAGE_NAME}_VERSION})")
endif()
